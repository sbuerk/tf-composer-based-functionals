{
  "name": "fes/composer-system-builder",
  "type": "library",
  "require": {
    "nesbot/carbon": "^2.63",
    "symfony/process": "^4.4 || ^5.4 || ^7",
    "thecodingmachine/safe": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "FES\\ComposerSystemBuilder\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FES\\ComposerSystemBuilder\\Tests\\": "Tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  }
}
